{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@content/*": ["src/content/*"],
      "@components/*": ["src/components/*"],
      "@scripts/*": ["src/scripts/*"],
      "@types/*": ["src/types/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
